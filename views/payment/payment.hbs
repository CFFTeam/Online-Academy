              <rect width="8" height="13" fill="white" />
            </clipPath>
          </defs>
        </svg>
        </img>
      </div>`)
    };
    const promotionList=[]
      
    promotionList.forEach(addPromotion)

    $(".deletePromotionItem").on("click", (event)=>{
      $(event.currentTarget).parent().remove(`${event.currentTarget.id}`);
      promotionList.remove(event.currentTarget.id) 
    });

    $(".promotionButton").click(function(){
        let bla = $('.promotionInput').val();
        if(bla.length>0){
           promotionList.push(bla);
          addPromotion(bla, promotionList.length - 1);

          $(`#${promotionList.length - 1} .deletePromotionItem`).on("click", (event)=>{
            $(event.currentTarget).parent().remove(`${event.currentTarget.id}`); 
            promotionList.splice(promotionList.indexOf(event.currentTarget.id),1) 
          });

          $('.promotionInput').val("");
        }
    });

    $('.promotionInput').keypress(function (e) {
        var key = e.which;
        if(key == 13)  // the enter key code
          {
            let bla = $('.promotionInput').val();
             if(bla.length>0){
              promotionList.push(bla);
              addPromotion(bla, promotionList.length - 1);

              $(`#${promotionList.length - 1} .deletePromotionItem`).on("click", (event)=>{
                $(event.currentTarget).parent().remove(`${event.currentTarget.id}`);
                promotionList.splice(promotionList.indexOf(event.currentTarget.id),1) 
              });

              $('.promotionInput').val("");
        }
          }
    });   

    const addItem=(item,index)=>{
      $(".itemList").append(`<div id=${index} class="item">
          <div class="image">
