                {{/times}}

                <svg width="16" height="14" viewBox="0 0 16 14" fill="none" xmlns="http://www.w3.org/2000/svg">
                  <g clip-path="url(#clip0_187_14473)">
                    <path
                      d="M10.5028 4.23281L14.7611 4.85352C15.0111 4.88906 15.2167 5.06133 15.2972 5.29648C15.375 5.53437 15.3111 5.79414 15.1333 5.96641L12.0444 8.97969L12.775 13.2316C12.8167 13.4777 12.7139 13.7293 12.5056 13.8742C12.3 14.0191 12.0028 14.0383 11.8028 13.9207L7.99722 11.9191L4.16944 13.9207C3.96944 14.0383 3.69722 14.0191 3.48889 13.8742C3.28333 13.7293 3.18056 13.4777 3.19722 13.2316L3.95 8.97969L0.864167 5.96641C0.684722 5.79414 0.621111 5.53437 0.7 5.29648C0.778611 5.06133 0.986111 4.88906 1.23583 4.85352L5.49167 4.23281L7.39722 0.369687C7.51111 0.143527 7.74167 0 7.99722 0C8.25278 0 8.48611 0.143555 8.59722 0.369687L10.5028 4.23281ZM7.975 10.5191C8.10833 10.5191 8.21389 10.5437 8.31111 10.5957L11.2333 12.1324L10.6722 8.86211C10.6361 8.65156 10.7083 8.43555 10.8611 8.28516L13.2472 5.9582L9.96111 5.48242C9.74167 5.44961 9.55278 5.31289 9.45833 5.11875L7.975 2.16262V10.5191Z"
                      fill="#FFF500" />
                  </g>
                  <defs>
                    <clipPath id="clip0_187_14473">
                      <rect width="16" height="14" fill="white" />
                    </clipPath>
                  </defs>
                </svg>
                <p>(${item.numReview})</p>
              </div>
              <p class="author">${item.author}</p>
            </div>
            <p class="view">${item.view} views</p>
            <p class="date">${item.date}</p>
            <p class="itemPrice">$${item.price}</p>
            <p class="bestseller">
              Bestseller
            </p>
          </div>
        </div>`)};

        
        const itemList = JSON.parse(`{{{course}}}`)

        if(itemList.length>0){
          $(".cartIsEmpty").addClass("shoppingCartHidden")
        }

        let total = 0;
        let discount = 0;
        for(let i=0;i<itemList.length;i++){
          total+=parseFloat(itemList[i].price);
          discount += parseFloat(itemList[i].discount);
        }
        if(total>0){
         $(".price > .value").text("$"+(Math.round(total * 100) / 100))
         $(".total > .value").text("$"+(Math.round((total+5-discount) * 100) / 100))
        }
        //(0%) - $0.00
        if(discount>0){
          $(".discount >.value").text("("+(Math.round((discount/total) * 10000) / 100)+"%) - $"+(Math.round(discount * 100) / 100))
        }
        
        itemList.forEach(addItem);
        
        let deleteForm = document.forms['deleteItem']

        $(".trashButton").on("click", (event)=>{
          $(event.currentTarget).parent().parent().remove(`${event.currentTarget.id}`); 
          itemList[$(event.currentTarget).parent().parent().attr('id')].price = 0;
          $(".getId").val(itemList[$(event.currentTarget).parent().parent().attr('id')].id)
          
          deleteForm.submit()
        });


        $(".checkoutButton").on("click",(event)=>{
          if(itemList.length>0){
          $(".paymentButton").removeClass("shoppingCartHidden")
          $(".checkoutButton").addClass("shoppingCartHidden")
          $(".itemList").addClass("shoppingCartHidden")
          $(".promotion").addClass("shoppingCartHidden")
          $(".titlee").addClass("shoppingCartHidden")
          $(".paymentMethod").removeClass("shoppingCartHidden")
          $(".Coupons").removeClass("shoppingCartHidden")
          $(".checkout").addClass("shoppingCartMargin")
          }
          else{
            alert("Shopping cart is empty")
          }
        })
          
